<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!--设置mybatis的输出日志-->
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"></setting>
    </settings>
    <typeAliases>
        <package name="com.gy.crm.settings.entity"/>
    </typeAliases>
    <!-- 映射器：指定映射文件或者映射类 -->
    <mappers>
        <package name="com.gy.crm.settings.dao"/>
    </mappers>
</configuration>